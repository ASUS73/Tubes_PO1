<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<!-- Tambahkan fx:controller dan fx:id pada elemen yang diperlukan -->
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="363.0" prefWidth="396.0"
            xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="app.tubes_po_gui_v1.ModalIsiSaldoController">
   <children>
      <Label layoutX="39.0" layoutY="35.0" text="Pilih Akun">
         <font>
            <Font name="Monospaced Regular" size="19.0" />
         </font>
      </Label>
      <!-- Tambahkan fx:id pada ChoiceBox -->
      <ChoiceBox fx:id="akunChoiceBox" layoutX="39.0" layoutY="64.0" prefHeight="25.0" prefWidth="315.0" />
      <!-- Tambahkan fx:id pada TextField -->
      <TextField fx:id="jumlahField" layoutX="39.0" layoutY="182.0" prefHeight="35.0" prefWidth="315.0" />
      <Label layoutX="39.0" layoutY="156.0" text="Jumlah Saldo ">
         <font>
            <Font name="Monospaced Regular" size="19.0" />
         </font>
      </Label>
      <!-- Tambahkan onAction pada Button -->
      <Button layoutX="159.0" layoutY="291.0" mnemonicParsing="false"
              onAction="#isiSaldo" prefHeight="35.0" prefWidth="69.0" text="Masukan">
         <font>
            <Font name="Monospaced Regular" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>

